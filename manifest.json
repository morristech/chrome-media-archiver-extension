{
    "name": "Video Archiver",
    "version": "1.0.0",
    "manifest_version": 2,
    "offline_enabled": false,
    "description": "...",
    "options_page": "options.html",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "notifications",
        "webRequest",
        "http://*/*"
    ],
    "background": {
        "scripts": ["background.js"]
    }
}
